  "scripts": {
    "initVscode": "yarn dlx @yarnpkg/sdks vscode",
    "dev": "NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "build": "NODE_ENV=production rm -rf ./build && yarn tsc --project tsconfig.prod.json",
    "start": "NODE_ENV=development yarn build && TS_NODE_BASEURL=./build node -r tsconfig-paths/register ./build/src/app.js",
    "test": "NODE_ENV=test ts-mocha --paths -file ./test/setups/index.ts ./test/**/*.test.ts --exit",
    "devTest": "NODE_ENV=test ts-mocha --paths -file ./test/setups/index.ts test/**/*.test.ts -w --watch-files '**/*.ts'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "compile:types": "tsc --noEmit"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "glob": "^9.2.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "typeorm": "^0.3.12",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-store": "^2.0.1"
  },
